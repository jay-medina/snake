{
  "name": "elm_version",
  "version": "1.0.0",
  "description": "## Background",
  "main": "index.js",
  "scripts": {
    "copy:static": "cp -R src/static/ dist/",
    "build:elm": "elm make src/Main.elm --output=dist/elm.js",
    "build": "run-s build:elm copy:static",
    "start:server": "elm-live src/Main.elm --output=dist/elm.js --port=8080 --dir=dist/",
    "start": "run-p copy:static start:server"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "elm-live": "^3.4.0",
    "npm-run-all": "^4.1.5"
  }
}
