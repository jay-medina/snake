{
  "extends": "tslint:recommended",
  "jsRules": {
    "quotemark": [true, "single"],
    "object-literal-sort-keys": false
  },
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "interface-name": [true, "never-prefix"],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "variable-name": false,
    "curly": false,
    "member-access": false,
    "semicolon": false
  },
  "linterOptions": {
    "exclude": ["coverage/**/*"]
  }
}
